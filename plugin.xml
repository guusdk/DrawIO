<?xml version="1.0" encoding="UTF-8" ?>
<plugin>
    <author>Ignite Realtime</author>
    <class>org.ifsoft.draw.openfire.PluginImpl</class>
    <name>Draw-IO</name>
    <description>Web Diagramming Tool that uses SVG and HTML for rendering</description>
    <licenseType>Apache 2.0</licenseType>
    <version>0.0.1</version>
    <date>10/21/2018</date>
    <minServerVersion>4.0.0</minServerVersion>

   <adminconsole>
        <tab id="tab-webclients">
            <sidebar id="siderbar-draw" name="${plugin.title}" description="${plugin.title.description}" >
                <item id="draw-settings" name="${config.page.settings}" description="${config.page.settings.description}" url="draw.jsp"/>
            </sidebar>
        </tab>
    </adminconsole> 
</plugin>
